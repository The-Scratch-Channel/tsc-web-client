{
  "threshold": 5,
  "reporters": ["html", "console", "json"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.min.js",
    "**/public/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "format": ["javascript", "jsx"],
  "minLines": 5,
  "minTokens": 50,
  "output": "./jscpd-report",
  "exitCode": 1
}
